{
  "manifest_version": 3,
  "name": "CatCat MaxImg Proxy",
  "version": "1.0",
  "description": "Proxy images and videos through CDN  with range request support",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "MaxImg Proxy"
  },
  "icons": {
    "16": "hat.png"
  }
}